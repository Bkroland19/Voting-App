generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model admin {
  firstName String
  lastName String
  userName String
  tel String
  organization String
  email    String @id
  password String
  id       String
}

model position {
  name String @unique @id
  user String
}

model Nominee {
  name  String
  post  String
  image String
  votes Int
  id    String @id
  postNo Int
  user String
}

model Coupons {
  codes String  @id
  used  Boolean
  user String
}
