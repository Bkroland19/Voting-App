generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model admin {
  email    String @id
  password String
  id       String
}

model Nominee {
  name  String
  post  String
  image String
  votes Int
  id    String @id
  postNo Int
}

model Coupons {
  codes String  @id
  used  Boolean
}
