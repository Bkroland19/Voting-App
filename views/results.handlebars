<div class="container">
    <div class="container p-3 text-center">
        <h2>Election Results</h2>
    </div>

    <div class="row">
        <table class=" table table-striped table-bordered">
            <thead class="thead-dark">
                <th scope="col">Post</th>
                <th scope="col">Nominee 1</th>
                <th scope="col">Nominee 2</th>
                <th scope="col">Nominee 3</th>
            </thead>
            <tbody>
                {{#each Results}}
                <tr>
                    <th scope="row">{{this.Category}}</th>
                    {{#each this.Nominees}}
                    {{!-- <tr scope="row"> --}}
                        <td>{{this.name}} - {{this.votes}}</td>
                    {{!-- </tr> --}}
                    
                    {{/each}}
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>